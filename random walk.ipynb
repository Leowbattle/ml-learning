   "source": [
    "n  = 100000\n",
    "nt = 1000\n",
    "\n",
    "points = np.zeros(n)\n",
    "\n",
    "for i in range(nt):\n",
    "    steps = np.random.choice([-1, 1], size=n)\n",
    "    points += steps\n",
    "\n",
    "plt.hist(points, bins=30, density=True, alpha=0.6, color='g')\n",
    "plt.show()"
   ]
